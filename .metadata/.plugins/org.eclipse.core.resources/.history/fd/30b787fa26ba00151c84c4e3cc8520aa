package Monday;

import static org.junit.Assert.*;

import org.junit.Test;

public class TestFileOperations {
	@Test
	public void testRead() {
		String expected = "as\n"
						+ "dgss df\n"
						+ "fdsg fsdh dsg\n" 
						+ "asf g\n"
						+ "sdfg\n" 
						+ "es hjrshjdfg\n"
						+ " agh\n"
						+ "sdg es\n"
						+ "gdffh 7  5247 y35\n"
						+ "aey3\n"
						+ "3 y357\n"
						+ "346 34 6\n"
						+ "37 3\n"  
						+ " 632 63";
		Path path = new Path();
		assertEquals("Reading is OK?", expected, readFrom(path));
	}
}
