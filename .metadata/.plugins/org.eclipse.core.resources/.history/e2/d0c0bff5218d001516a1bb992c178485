	public static int getOddOccurrence(int[] array){
		long aB = 1;
		if(b == 0){
			return 1;
		}
		if(b == 1){
			return a;
		}
		aB = pow(a, b/2);
		aB *= aB;
		if(b % 2 == 1){
			aB *= a;
		}
		return aB;
	}
